<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.androidfuture.newyear.framesfree"
    android:versionCode="1"
	android:versionName="1.0"
    android:installLocation="preferExternal"
>

    <uses-sdk android:minSdkVersion="7" />

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.SET_WALLPAPER"/>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>
<uses-permission android:name="android.permission.INTERNET" />
<uses-permission android:name="android.permission.READ_PHONE_STATE"/>
    <application
        android:icon="@drawable/icon_launch"
        android:label="@string/app_name" android:name=".FrameApp">
        <activity
            android:label="@string/app_name"
            android:name="com.androidfuture.frames.ui.ProcessActivity" >
            <intent-filter >
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        
            <activity android:name="com.appdao.android.feedback.FeedBack"></activity>
 		<activity android:name="com.google.ads.AdActivity"
        android:configChanges="keyboard|keyboardHidden|orientation|screenLayout|uiMode|screenSize|smallestScreenSize"/>  <activity android:name="com.androidfuture.frames.ui.SetActivity"></activity>
  <activity android:name="com.androidfuture.frames.ui.PreviewActivity"></activity>
  <activity android:name="com.androidfuture.facebook.PhotoActivity"></activity>
  <activity android:name="com.androidfuture.facebook.PhotoExplore"></activity>
  <activity android:name="com.androidfuture.facebook.PhotoSelectActivity"></activity>

</application>
</manifest>